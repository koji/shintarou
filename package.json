{
  "name": "shintarou",
  "private": false,
  "version": "0.0.35",
  "type": "module",
  "description": "shintarou/新太郎 is a react component that helps to create a layout easily. It supports normal div, flexbox and grid.",
  "scripts": {
    "build": "pnpm run clean && tsc && vite build",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "cz": "git-cz",
    "storybook": "storybook dev",
    "build-storybook": "storybook build -o docs-build",
    "test": "vitest",
    "test-run": "vitest run",
    "coverage": "vitest run --coverage",
    "clean": "shx rm -rf dist types"
  },
  "engines": {
    "node": ">=18.16.1"
  },
  "packageManager": "pnpm@8.15.4",
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@storybook/addon-actions": "^7.6.20",
    "@storybook/addon-essentials": "^7.6.20",
    "@storybook/addon-interactions": "^7.6.20",
    "@storybook/addon-links": "^7.6.20",
    "@storybook/react": "^7.6.20",
    "@storybook/react-vite": "^7.6.20",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^14.3.1",
    "@types/lodash": "^4.17.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.57.1",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-testing-library": "^6.3.4",
    "git-cz": "^4.9.0",
    "jsdom": "^24.1.3",
    "json": "^11.0.0",
    "prettier": "^3.3.3",
    "react-hooks": "^1.0.1",
    "shx": "^0.3.4",
    "storybook": "^7.6.20",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-plugin-compression2": "^0.12.0",
    "vite-plugin-dts": "^4.0.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "lodash": "^4.17.0",
    "react": "^16.8.0 || 18.x",
    "react-dom": "^16.8.0 || 18.x",
    "styled-components": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/shintarou.umd.js",
  "module": "./dist/shintarou.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/shintarou.es.js",
      "require": "./dist/shintarou.umd.js"
    }
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "flexbox",
    "grid",
    "layout"
  ],
  "author": {
    "name": "koji kanao"
  },
  "bugs": {
    "url": "https://github.com/koji/shintarou/issues"
  },
  "homepage": "https://github.com/koji/shintarou",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koji/shintarou"
  }
}
